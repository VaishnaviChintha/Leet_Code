import java.util.*;
public class Solution {
    public int countDistinctIntegers(int[] nums) {
        Set<Integer> res = new HashSet<>();
        for(int num : nums){
            res.add(num);
            res.add(help(num));
        }
        return res.size();
    }
    private static int help(int num){
        int res=0;
        while(num!=0){
           int digit = num%10;
           res = res*10+digit;
           num/=10;
        }
        return res;
    }
public static void main(String[] args) {
        Solution sol = new Solution();
        int[] nums = {1, 13, 10, 12, 31};
        int result = sol.countDistinctIntegers(nums);
        System.out.println("Count of distinct integers: " + result);
    }
}
