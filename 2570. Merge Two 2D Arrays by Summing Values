import java.util.*;
public class Solution {
    public int[][] mergeArrays(int[][] nums1, int[][] nums2) {
        Map<Integer,Integer> freq = new TreeMap<>();
        for(int i=0;i<nums1.length;i++){
            freq.put(nums1[i][0],freq.getOrDefault(nums1[i][0],0)+nums1[i][1]);
        }
        for(int i=0;i<nums2.length;i++){
            freq.put(nums2[i][0],freq.getOrDefault(nums2[i][0],0)+nums2[i][1]);
        }
        int[][] res = new int[freq.size()][2];
        int idx=0;
        for(Map.Entry<Integer,Integer> entry : freq.entrySet()){
             res[idx][0]=entry.getKey();
             res[idx][1]=entry.getValue();
             idx++;
        }
        return res;
    }
public static void main(String[] args) {
        Solution sol = new Solution();
        int[][] nums1 = {{1, 2}, {2, 3}, {4, 5}};
        int[][] nums2 = {{1, 4}, {3, 2}, {4, 1}};
        int[][] result = sol.mergeArrays(nums1, nums2);
        for (int[] row : result) {
            System.out.println(row[0] + " " + row[1]);
        }
}
