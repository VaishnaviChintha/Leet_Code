import java.util.*;
public class Solution {
    public int partitionString(String s) {
       int count=0;
       Set<Character> set = new HashSet<>();
       for(char ch : s.toCharArray()){
        if(set.contains(ch)){
            set.clear();
            count++;
        }
        set.add(ch);
       } 
      return count+1;
    }
public static void main(String[] args) {
        Solution sol = new Solution();
        String s1 = "abacaba";
        System.out.println(sol.partitionString(s1)); 
        String s2 = "ssssss";
        System.out.println(sol.partitionString(s2)); 
        String s3 = "abcde";
        System.out.println(sol.partitionString(s3)); 
    }
    
}
