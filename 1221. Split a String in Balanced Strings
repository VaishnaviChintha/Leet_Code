import java.util.*;
public class Solution {
    public int balancedStringSplit(String s) {
        int left=0;
        int count=0;
        int parity=0;
        for(int right=0;right<s.length();right++){
            if(s.charAt(right)=='R'){
                 parity+=1;
            }else{
                parity-=1;
            }
            if(parity==0){
                left=right;
                parity=0;
                count++;
            }
            
        }
        return count;
    }
public static void main(String[] args) {
        Solution sol = new Solution();
        String s1 = "RLRRLLRLRL";
        System.out.println(sol.balancedStringSplit(s1));
        String s2 = "RLLLLRRRLR";
        System.out.println(sol.balancedStringSplit(s2)); 
    }
}
